{
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "node16",
    "moduleResolution": "node16",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "strictFunctionTypes": false,
    "strictNullChecks": false,
    "strictBindCallApply": false,
    "strictBuiltinIteratorReturn": false,
    "allowImportingTsExtensions": true,
    "resolvePackageJsonImports": true,
    "resolveJsonModule": true,
    "allowArbitraryExtensions": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true,
    "allowUnusedLabels": true,
    "allowUnreachableCode": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "config/*": ["./src/config/*"],
      "library/*": ["./src/library/*"],
      "server/*": ["./src/server/*"],
      "data/*": ["./src/server/data/*"],
      "db": ["./src/config/pglite.ts"],
      "env": ["./src/config/env.ts"],
      "app": ["src/server/index.ts"],
      "constant": ["./src/library/constants.ts"],
      "variables": ["src/library/variables.ts"],
      "utils": ["src/library/templates/index.ts"],
      "deps": ["./src/config/deps.ts"],
      "initializer": ["src/modules/server/index.ts"]
    }
  }
}